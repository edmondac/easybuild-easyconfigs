easyblock = "Toolchain"

name = 'gmpich'
version = '2016a'

homepage = '(none)'
description = """gcc and GFortran based compiler toolchain,
 including MPICH for MPI support."""

toolchain = {'name': 'dummy', 'version': 'dummy'}

compname = 'GCC'
compver = '4.9.3-2.25'
comp = (compname, compver)

mpilib = 'MPICH'
mpiver = '3.2'

# compiler toolchain dependencies
dependencies = [
    comp,
    (mpilib, mpiver, '', comp),
]

moduleclass = 'toolchain'
